# Scheduling & backfill
SCHED_INTERVAL=20
ENABLE_BACKFILL=Y
BACKFILL_POLICY=BESTFIT

# Fairshare
FAIRSHARE=USER
FAIRSHARE_FILE=/prj/ykb/conf/ykb/lsb.users
FS_DECAY_INTERVAL=24h
FS_DECAY_FACTOR=0.5
FS_WEIGHT=PRIORITY

# Preemption
PREEMPT_POLICIES=QUEUE
PREEMPTION_FILE=/prj/ykb/conf/ykb/lsb.preempt
PREEMPT_INTERVAL=60
PREEMPT_ENFORCE_ORDER=Y
PREEMPT_REQUEUE=Y
PREEMPT_SUSPEND_TIMEOUT=300
MAX_PREEMPTIONS_PER_CYCLE=10
